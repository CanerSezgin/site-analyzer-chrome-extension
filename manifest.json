{
  "manifest_version": 2,
  "name": "Website Analyzer",
  "version": "1.0.0",
  "description": "Analyze Any Website | SEO",
  "author": "Caner Sezgin",
  "permissions": ["storage", "activeTab", "declarativeContent"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
      "default_popup": "index.html"
  }
}
